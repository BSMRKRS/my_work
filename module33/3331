import RoboPiLib as RPL
import setup
import time
sensor_pin = 18
RPL.pinMode(sensor_pin,RPL.INPUT)

running = True
while running:
	reading=RPL.digitalRead(sensor_pin)
	if reading==0:
		RPL.servoWrite(1,00)		
		running=False
	RPL.servoWrite(1,1600)
	time.sleep(1)
	RPL.servoWrite(1,00)
	time.sleep(1)
	
	
	




