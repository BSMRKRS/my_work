if [ "$1" == "flast00" ]; then
  echo "You cannot use that branch name! Please check it and try again.";
elif [ "$1" == "master" ]; then
  echo "You cannot use that branch name! Please check it and try again.";
elif [ "$1" == "pkirwin" ]; then
  echo "You cannot use that branch name! Please check it and try again.";
else
  git config user.email "$1@bsmschool.org";
  git config user.name $1;
  git checkout -b $1;
fi
